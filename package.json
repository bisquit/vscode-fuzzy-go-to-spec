{
  "name": "vscode-fuzzy-go-to-spec",
  "displayName": "Fuzzy Go to Spec",
  "description": "Switch between source and spec using fuzzy search",
  "version": "0.0.1",
  "license": "MIT",
  "publisher": "bisquit",
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "contributes": {
    "commands": [
      {
        "command": "fuzzy-go-to-spec.goToSpec",
        "title": "Go to Spec",
        "category": "Fuzzy Go to Spec"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run esbuild-base --minify",
    "esbuild-base": "rimraf dist && esbuild ./src/index.ts --bundle --outfile=dist/index.js --external:vscode --format=cjs --platform=node",
    "esbuild": "pnpm run esbuild-base --sourcemap",
    "esbuild:watch": "pnpm run esbuild-base --sourcemap --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "vsce:package": "pnpm run vsce package --no-dependencies",
    "vsce:publish": "pnpm run vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.26.0",
    "rimraf": "^5.0.1",
    "tsx": "^3.12.7",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "fast-fuzzy": "^1.12.0"
  },
  "prettier": {
    "editorconfig": true,
    "singleQuote": true
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-non-null-assertion": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bisquit/vscode-fuzzy-go-to-spec.git"
  },
  "bugs": {
    "url": "https://github.com/bisquit/vscode-fuzzy-go-to-spec/issues"
  }
}
